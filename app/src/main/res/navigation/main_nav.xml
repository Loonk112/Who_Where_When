        <action
            android:id="@+id/action_MVTGVDetailsView_to_MVTGVDVEVAddEmployeeView"
            app:destination="@id/MVTGVDVEVAddEmployeeView" />
        <action
            android:id="@+id/action_MVTGVDetailsView_to_MVTGVDVTVAddTaskView"
            app:destination="@id/MVTGVDVTVAddTaskView" />
        <action
            android:id="@+id/action_MVTGVDetailsView_to_MVTGVDVTVTimeView"
            app:destination="@id/MVTGVDVTVTimeView" />
        <action
            android:id="@+id/action_MVTGVDetailsView_to_MVTGVDVEVTimeView"
            app:destination="@id/MVTGVDVEVTimeView" />
    </fragment>
    <fragment
        android:id="@+id/MVEVDetailsView"
        android:name="com.example.whowherewhen.MVEmployeeVDetailsView"
        android:label="fragment_m_v_u_v_details_view"
        tools:layout="@layout/fragment_m_v_e_v_details_view" >
