            app:destination="@id/MVTGVDetailsView" />
    </fragment>
    <fragment
        android:id="@+id/MVTGVDVTVTimeView"
        android:name="com.example.whowherewhen.MVTGVDVTVTimeView"
        android:label="fragment_m_v_t_g_v_d_v_t_v_time_view"
        tools:layout="@layout/fragment_m_v_t_g_v_d_v_t_v_time_view" >
        <action
            android:id="@+id/action_MVTGVDVTVTimeView_to_MVTGVDetailsView"
            app:destination="@id/MVTGVDetailsView" />
    </fragment>
    <fragment
        android:id="@+id/MVTGVDVEVTimeView"
        android:name="com.example.whowherewhen.MVTGVDVEVTimeView"
        android:label="fragment_m_v_t_g_v_d_v_e_v_time_view"
        tools:layout="@layout/fragment_m_v_t_g_v_d_v_e_v_time_view" >
        <action
            android:id="@+id/action_MVTGVDVEVTimeView_to_MVTGVDetailsView"
            app:destination="@id/MVTGVDetailsView" />
    </fragment>
</navigation>