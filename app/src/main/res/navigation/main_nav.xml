<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav"
    app:startDestination="@id/loginView">

    <fragment
        android:id="@+id/loginView"
        android:name="com.example.whowherewhen.LoginView"
        android:label="fragment_login_view"
        tools:layout="@layout/fragment_login_view" >
        <action
            android:id="@+id/action_loginView_to_managerView"
            app:destination="@id/managerView" />
    </fragment>
    <fragment
        android:id="@+id/managerView"
        android:name="com.example.whowherewhen.ManagerView"
        android:label="fragment_manager_view"
        tools:layout="@layout/fragment_manager_view" >
        <action
            android:id="@+id/action_managerView_to_loginView"
            app:destination="@id/loginView" />
        <action
            android:id="@+id/action_managerView_to_MVUVAddUserView"
            app:destination="@id/MVUVAddUserView" />
        <action
            android:id="@+id/action_managerView_to_MVTGVAddTaskGroupView"
            app:destination="@id/MVTGVAddTaskGroupView" />
        <action
            android:id="@+id/action_managerView_to_MVTGVDetailsView"
            app:destination="@id/MVTGVDetailsView" />
        <action
            android:id="@+id/action_managerView_to_MVUVDetailsView"
            app:destination="@id/MVUVDetailsView" />
    </fragment>
    <fragment
        android:id="@+id/MVUsersView"
        android:name="com.example.whowherewhen.MVUsersView"
        android:label="fragment_m_v_users_view"
        tools:layout="@layout/fragment_m_v_users_view" />
    <fragment
        android:id="@+id/MVTaskGroupsView"
        android:name="com.example.whowherewhen.MVTaskGroupsView"
        android:label="fragment_m_v_task_groups_view"
        tools:layout="@layout/fragment_m_v_task_groups_view" />
    <fragment
        android:id="@+id/MVUVAddUserView"
        android:name="com.example.whowherewhen.MVUVAddUserView"
        android:label="fragment_m_v_u_v_add_user_view"
        tools:layout="@layout/fragment_m_v_u_v_add_user_view" >
        <action
            android:id="@+id/action_MVUVAddUserView_to_managerView"
            app:destination="@id/managerView" />
    </fragment>
    <fragment
        android:id="@+id/MVTGVAddTaskGroupView"
        android:name="com.example.whowherewhen.MVTGVAddTaskGroupView"
        android:label="fragment_m_v_t_g_v_add_task_group_view"
        tools:layout="@layout/fragment_m_v_t_g_v_add_task_group_view" >
        <action
            android:id="@+id/action_MVTGVAddTaskGroupView_to_managerView"
            app:destination="@id/managerView" />
    </fragment>
    <fragment
        android:id="@+id/MVTGVDetailsView"
        android:name="com.example.whowherewhen.MVTGVDetailsView"
        android:label="fragment_m_v_t_g_v_details_view"
        tools:layout="@layout/fragment_m_v_t_g_v_details_view" >
        <action
            android:id="@+id/action_MVTGVDetailsView_to_managerView"
            app:destination="@id/managerView" />
    </fragment>
    <fragment
        android:id="@+id/MVUVDetailsView"
        android:name="com.example.whowherewhen.MVUVDetailsView"
        android:label="fragment_m_v_u_v_details_view"
        tools:layout="@layout/fragment_m_v_u_v_details_view" >
        <action
            android:id="@+id/action_MVUVDetailsView_to_managerView"
            app:destination="@id/managerView" />
    </fragment>
    <fragment
        android:id="@+id/MVTGVDVEmployeesView"
        android:name="com.example.whowherewhen.MVTGVDVEmployeesView"
        android:label="fragment_m_v_t_g_v_d_v_employees_view"
        tools:layout="@layout/fragment_m_v_t_g_v_d_v_employees_view" />
    <fragment
        android:id="@+id/MVTGVDVTasksView"
        android:name="com.example.whowherewhen.MVTGVDVTasksView"
        android:label="fragment_m_v_t_g_v_d_v_tasks_view"
        tools:layout="@layout/fragment_m_v_t_g_v_d_v_tasks_view" />
</navigation>