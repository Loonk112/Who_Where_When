<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav"
    app:startDestination="@id/loginView">

    <fragment
        android:id="@+id/loginView"
        android:name="com.example.whowherewhen.LoginView"
        android:label="fragment_login_view"
        tools:layout="@layout/fragment_login_view" >
        <action
            android:id="@+id/action_loginView_to_managerView"
            app:destination="@id/managerView" />
    </fragment>
    <fragment
        android:id="@+id/managerView"
        android:name="com.example.whowherewhen.ManagerView"
        android:label="fragment_manager_view"
        tools:layout="@layout/fragment_manager_view" >
        <action
            android:id="@+id/action_managerView_to_loginView"
            app:destination="@id/loginView" />
        <action
            android:id="@+id/action_managerView_to_MVEVAddEmployeeView"
            app:destination="@id/MVEVAddEmployeeView" />
        <action
            android:id="@+id/action_managerView_to_MVTGVAddTaskGroupView"
            app:destination="@id/MVTGVAddTaskGroupView" />
        <action
            android:id="@+id/action_managerView_to_MVTGVDetailsView"
            app:destination="@id/MVTGVDetailsView" />
        <action
            android:id="@+id/action_managerView_to_MVEVDetailsView"
            app:destination="@id/MVEVDetailsView" />
    </fragment>
    <fragment
        android:id="@+id/MVEmployeesView"
        android:name="com.example.whowherewhen.MVEmployeesView"
        android:label="fragment_m_v_users_view"
        tools:layout="@layout/fragment_m_v_employees_view" />
